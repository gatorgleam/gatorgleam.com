        position: sticky;
        top: 0;
        z-index: 10;
        backdrop-filter: blur(8px);
      }
      nav ul {
        display: flex;
        justify-content: center;
        gap: 3rem;
        list-style: none;
      }
      nav a {
        text-decoration: none;
        color: #1a1a1a;
        font-weight: 600;
        font-size: 1.1rem;
        transition: all 0.3s ease;
        padding: 0.5rem 1rem;
        border-radius: 30px;
      }
      nav a:hover {
        color: #ff69b4;
        background: linear-gradient(135deg, rgba(255,105,180,0.1), rgba(138,43,226,0.1));
      }
      
      .service-header {
        background: linear-gradient(135deg, #ff69b4, #8a2be2);
        padding: 8rem 2rem;
        text-align: center;
        color: white;
        position: relative;
        clip-path: polygon(0 0, 100% 0, 100% 85%, 0 100%);
      }
      .service-header h1 {
        font-size: 3.5rem;
        margin-bottom: 1.5rem;
        animation: slideUp 1s ease-out forwards;
      }
      .service-icon {
        font-size: 4rem;
        margin-bottom: 2rem;
        animation: fadeIn 1s ease-out;
      }
      .service-content {
        max-width: 1000px;
        margin: 4rem auto;
        padding: 0 2rem;
      }
      .feature-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
        gap: 2rem;
        margin: 3rem 0;
      }
      .feature-card {
        background: white;
        padding: 2rem;
        border-radius: 16px;
        box-shadow: 0 8px 24px rgba(0,0,0,0.08);
        transition: all 0.3s ease;
      }
      .feature-card:hover {
        transform: translateY(-5px);
        box-shadow: 0 12px 28px rgba(0,0,0,0.12);
      }
      .feature-card h3 {
        color: #ff69b4;
        margin-bottom: 1rem;
        font-size: 1.3rem;
      }
      .feature-card p {
        color: #666;
      }
      .workflow-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
        gap: 2rem;
        margin: 3rem 0;
      }
      .workflow-step {
        text-align: center;
        padding: 2rem;
        background: linear-gradient(135deg, rgba(255,105,180,0.1), rgba(138,43,226,0.1));
        border-radius: 16px;
        position: relative;
      }
      .workflow-step::after {
        content: '→';
        position: absolute;
        right: -1rem;
        top: 50%;
        transform: translateY(-50%);
        color: #ff69b4;
        font-size: 1.5rem;
      }
      .workflow-step:last-child::after {
        display: none;
      }
      .workflow-icon {
        font-size: 2rem;
        color: #ff69b4;
        margin-bottom: 1rem;
      }
      .process-section {
        margin: 5rem 0;
      }
      .process-step {
        display: flex;
        align-items: flex-start;
        margin-bottom: 3rem;
        opacity: 0;
        animation: fadeInUp 0.6s ease-out forwards;
      }
      .process-step:nth-child(1) { animation-delay: 0.2s; }
      .process-step:nth-child(2) { animation-delay: 0.4s; }
      .process-step:nth-child(3) { animation-delay: 0.6s; }
      .process-step:nth-child(4) { animation-delay: 0.8s; }
      
      .step-number {
        font-size: 2rem;
        font-weight: bold;
        color: #ff69b4;
        margin-right: 2rem;
        min-width: 40px;
      }
      .step-content h3 {
        font-size: 1.5rem;
        margin-bottom: 1rem;
        color: #333;
      }
      .step-content p {
        color: #666;
      }
      @keyframes slideUp {
        from {
          opacity: 0;
          transform: translateY(30px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }
      @keyframes fadeIn {
        from { opacity: 0; }
        to { opacity: 1; }
      }
      @keyframes fadeInUp {
        from {
          opacity: 0;
          transform: translateY(20px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }
    </style>
  </head>
